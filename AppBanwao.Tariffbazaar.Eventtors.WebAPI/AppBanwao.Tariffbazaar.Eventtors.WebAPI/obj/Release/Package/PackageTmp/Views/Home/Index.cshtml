<input type="button" id="submit" value="Submit" />
<script>

    $(document).ready(function () {
        $("#submit").click(function () {
            var place = new Object();
            place.PlaceID = '@Guid.NewGuid()';
            place.Name = 'test garden';
            place.SeatingCapacity = 100;
            place.DiningCapacity = 100;
            place.Area = 100010;
            place.AvailableFrom = '@DateTime.Now.ToShortDateString()';
            place.AvailableTill = '@DateTime.Now.ToShortDateString()';
            place.PlaceType = 1;
            place.Active = true;
            place.CreatedDate = '@DateTime.Now.ToShortDateString()';
            place.UpdatedDate = '@DateTime.Now.ToShortDateString()';
            var placeAddresses = new Object();
           
            placeAddresses.placeID = '';
            placeAddresses.Addressline1 = 'test line 1';
            placeAddresses.Addressline2 = 'test line 2';
            placeAddresses.LandMark = 'test land mark';
            placeAddresses.CityID = 1;
            placeAddresses.StateID = 1;
            placeAddresses.Country = 1;
            place.PlaceAddresses = placeAddresses;


            $.ajax({
                url: 'http://localhost:55500/api/Place',
                type: 'POST',
                dataType: 'json',
                data: place,
                success: function (data, textStatus, xhr) {
                    if (data == true)
                    {
                        alert('Place added successfully');
                        window.location.reload(true);
                    }
                    //console.log(data);
                },
                error: function (xhr, textStatus, errorThrown) {
                    console.log('Error in Operation');
                }
            });


        });
    });
</script>
